{
  "name": "Project",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "accounts": {
      "type": "hasAndBelongsToMany",
      "model": "Account",
      "foreignKey": ""
    },
    "meetings": {
      "type": "hasMany",
      "model": "Meeting",
      "foreignKey": ""
    },
    "steps": {
      "type": "hasMany",
      "model": "Step",
      "foreignKey": ""
    },
    "workAreas": {
      "type": "hasMany",
      "model": "WorkArea",
      "foreignKey": ""
    },
    "roles": {
      "type": "hasMany",
      "model": "ProjectRole",
      "foreignKey": ""
    },
    "bugs": {
      "type": "hasMany",
      "model": "Bug",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "principalType":"ROLE",
      "principalId":"$everyone",
      "permission":"DENY"
    },
    {
      "property": "__get__workers",
      "principalType":"ROLE",
      "principalId":"$authenticated",
      "permission":"ALLOW"
    },
    {
      "property": "__get__roles",
      "principalType":"ROLE",
      "principalId":"$authenticated",
      "permission":"ALLOW"
    }
  ],
  "methods": {}
}
