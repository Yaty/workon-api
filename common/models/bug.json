{
  "name": "Bug",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "default": "Ouvert"
    },
    "description": {
      "type": "string",
      "required": true
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "project": {
      "type": "belongsTo",
      "model": "Project",
      "foreignKey": ""
    },
    "creator": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "creatorId"
    },
    "assignees": {
      "type": "hasAndBelongsToMany",
      "model": "Account",
      "foreignKey": ""
    },
    "messages": {
      "type": "hasMany",
      "model": "Message",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
